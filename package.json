{
  "name": "vandelay",
  "version": "8.11.1",
  "description": "Imports, exports, and ETL",
  "main": "dist/index.js",
  "keywords": [
    "etl",
    "pipeline",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/vandelay.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/vandelay/issues"
  },
  "homepage": "https://github.com/staeco/vandelay#readme",
  "files": [
    "dist"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": true
  },
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src tests --fix",
    "test:plain": "NODE_ENV=testing mocha tests --require @babel/register --recursive --reporter spec --bail --exit --timeout 90000",
    "test": "NODE_ENV=testing nyc mocha tests --require @babel/register --recursive --reporter spec --bail --exit --timeout 90000 -- --trace-warnings --trace-deprecation",
    "test:debug": "NODE_ENV=testing nyc mocha tests --inspect-brk=0.0.0.0 --require @babel/register --recursive --reporter spec --bail --timeout 90000 -- --trace-warnings --trace-deprecation"
  },
  "engines": {
    "node": ">=13"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@fast-csv/format": "^4.3.5",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@stae/babel-node": "^1.0.0",
    "@stae/linters": "^1.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "^1.18.3",
    "eslint": "^7.0.0",
    "express": "^4.16.3",
    "get-port": "^5.0.0",
    "graceful-fs": "^4.1.11",
    "into-stream": "^6.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.0",
    "range-parser": "^1.2.0",
    "rimraf": "^3.0.0",
    "should": "^13.0.0",
    "tempfile": "^3.0.0",
    "vandelay-es6": "^2.0.0",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "csv-parser": "^3.0.0",
    "duplexify": "^4.0.0",
    "get-stream": "^6.0.0",
    "got-resume-next": "^2.1.1",
    "gtfs-stream": "^2.1.0",
    "is-plain-obj": "^3.0.0",
    "jsonstream-next": "^3.0.0",
    "jsonstream-paths": "^1.1.1",
    "lodash": "^4.17.15",
    "moize": "^6.0.0",
    "moment": "^2.24.0",
    "ndjson": "^2.0.0",
    "object-transform-stack": "^3.0.0",
    "p-series": "^2.1.0",
    "p-timeout": "^4.0.0",
    "parse-decimal-number": "^1.0.0",
    "pipeline-pipe": "^0.3.0",
    "pumpify": "^2.0.0",
    "qs": "^6.6.0",
    "remove-bom-stream": "^2.0.0",
    "stream-exhaust": "^1.0.2",
    "superagent": "^7.0.0",
    "threads": "^1.6.3",
    "tough-cookie": "^4.0.0",
    "unzipper": "github:ZJONSSON/node-unzipper",
    "url-template": "^2.0.8",
    "verrazzano": "^2.0.1",
    "vm2": "^3.6.0",
    "wkx": "^0.5.0",
    "xlsx-parse-stream": "^1.0.0",
    "xml2js": "^0.4.19"
  },
  "resolutions": {
    "unzipper": "github:ZJONSSON/node-unzipper"
  },
  "babel": {
    "extends": "@stae/babel-node",
    "env": {
      "testing": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
